{
  "agent": {
    "name": "Data Analysis Agent",
    "description": "E-commerce data analysis specialist",
    "version": "1.0.0"
  },
  "llm": {
    "use_ensemble": true,
    "primary": {
      "provider": "ollama",
      "model": "llama3.2",
      "temperature": 0.1,
      "max_tokens": 2048
    },
    "secondary": {
      "provider": "google",
      "model": "gemini-2.5-flash",
      "temperature": 0.1,
      "max_tokens": 2048
    },
    "fallback": {
      "provider": "aws_bedrock",
      "model": "anthropic.claude-3-sonnet-20240229-v1:0",
      "temperature": 0.1,
      "max_tokens": 2048
    }
  },
  "memory": {
    "use_context": true,
    "max_context_items": 5,
    "similarity_threshold": 0.7,
    "conversation_history_limit": 10
  },
  "bigquery": {
    "project_id": "${GCP_PROJECT_ID}",
    "dataset": "bigquery-public-data.thelook_ecommerce",
    "location": "us-central1",
    "max_results": 10000,
    "timeout_seconds": 300,
    "available_tables": ["users", "orders", "order_items", "products", "inventory_items", "distribution_centers", "events"]
  },
  "behavior": {
    "analysis_style": "comprehensive",
    "report_format": "business_insights",
    "chart_generation": false,
    "max_retries": 3,
    "enable_caching": true
  }
}
